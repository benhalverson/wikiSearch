{
  "expo": {
    "name": "Wiki Search",
    "slug": "wikiSearch",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "me.benhalverson.wikisearch"
    },
    "android": {
      "package": "me.benhalverson.wikisearch"
    },
    "description": "A demo app that searches wikipedia",
    "githubUrl": "https://github.com/benhalverson/wikiSearch"
  }
}
